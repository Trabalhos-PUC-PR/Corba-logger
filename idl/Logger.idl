enum Severidade {
	DEBUG,
	WARNING,
	ERROR,
	CRITICAL
};

interface Logger {

	attribute boolean verbose;
	attribute readonly unsigned short count;

	void log(
		in Severidade severidade,
		in string endereco,
		in unsigned short pid,
		in string hora,
		in string msg
		);

};

